<%= form_for [:admin, @registration_form], multipart: true do |f| %>

  <% if @registration_form.errors.any? %>
    <div id="error_explanation">
      <p>
        <%= pluralize(@registration_form.errors.count, "error") %> prevented this registration form from being saved:
      </p>
      <ul>
        <% @registration_form.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="row">
    <div class="medium-12 columns">
      <%= f.label :year, "Year:" %>
      <%= f.text_field :year, placeholder: "20XX-20XX" %>
    </div>
  </div>

  <div class="row">
    <div class="medium-12 columns">
      <%= f.file_field :file %>
    </div>
  </div>

  <br>

  <div class="row">
    <div class="medium-2 columns">
      <%= f.submit "Submit" %>
    </div>
    <div class="medium-2 columns">
      <%= link_to "Cancel", admin_registration_forms_path %>
    </div>
  </div>
<% end %>